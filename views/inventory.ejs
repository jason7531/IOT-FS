<%- include("partials/header") %>
    <h1>Inventory by
        <%= inventory.author.username %>
    </h1>
    <p>List of products</p>
    <br />

    <% inventory.products.forEach(function(product) { %>
        <div class="container">
            <div class="card">
                <img src="<%= product.image %>" alt="" class="card-img-top" />
                <div class="card-body">
                    <h5 class="card-title">
                        <%= product.name %>
                    </h5>
                </div>
                <a href="/inventory/<%= inventory._id %>/edit" class="btn btn-warning">Edit Order</a
    >
  </div>
</div>

<% }) %> <%- include("partials/footer") %>